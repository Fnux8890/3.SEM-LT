{
    "presets": ["@babel/preset-env"],
    "plugins": [
        ["@babel/plugin-transform-runtime",{
            "absoluteRuntime": false,
            "corejs": false,
            "helpers": true,
            "regenerator": true
          }
        ],
        ["module-resolver", {
          "root": ["./"],
          "alias": {
            "@middleware": "./server/app/controller/middleware",
            "@models": "./server/app/models",
            "@public": "./server/app/views/",
            "@distCss": "./server/app/views/dist/css",
            "@publicCss": "./server/app/views/assets/scss"
          }
        }],
        ["@babel/plugin-syntax-dynamic-import"]
      ]
}