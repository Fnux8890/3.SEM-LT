{
    "presets": ["@babel/preset-env"],
    "plugins": [
        ["@babel/plugin-transform-runtime",{
            "absoluteRuntime": false,
            "corejs": false,
            "helpers": true,
            "regenerator": true
          }
        ],
        ["module-resolver", {
          "root": ["./"],
          "alias": {
            "@middleware": "./app/controller/middleware",
            "@models": "./app/models",
            "@public": "./app/views/*"
          }
        }]
      ]
}